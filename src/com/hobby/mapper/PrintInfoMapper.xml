<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.hobby.mapper.PrintInfoMapper">
 
 	<insert id="insertPost" parameterType="com.hobby.pojo.Post">
 		insert into post (title,time,content,uid,tid,id,hot)
 			values(#{title},#{time},#{content},#{uid.username},#{tid.id},#{id},#{hot})
 	</insert>
 	
 </mapper>